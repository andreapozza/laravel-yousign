{
    "name": "andreapozza/laravel-yousign",
    "description": "YouSign connector for Laravel",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Andreapozza\\LaravelYousign\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Andrea Pozza",
            "email": "andrea.pozza@hotmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0|^11.0",
        "andreapozza/yousign-php": "self.version"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Andreapozza\\LaravelYousign\\YouSignServiceProvider"
            ],
            "aliases": {
                "YouSign": "Andreapozza\\LaravelYousign\\Facades\\YouSign"
            }
        }
    },
    "repositories": {
        "andreapozza/yousign-php": {
            "type": "vcs",
            "url": "https://github.com/andreapozza/yousign-php"
        }
    }
}
